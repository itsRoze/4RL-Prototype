{
  "name": "@beta/supabase",
  "scripts": {
    "start": "supabase status || supabase start",
    "reset": "supabase db reset || supabase start",
    "test": "supabase db test",
    "lint": "supabase db lint",
    "deploy": "supabase link --project-ref $SUPABASE_PROJECT_REF && supabase db push",
    "sb:start": "supabase status || supabase start",
    "sb:stop": "supabase stop",
    "sb:types": "supabase gen types typescript --local > ./types/database-types.ts && prettier --write \"./types/**/*.{ts,tsx,md}\"",
    "sb:pull-local": "supabase db pull --local",
    "sb:pull-prod": "supabase db pull --linked",
    "sb:push-local": "supabase db push --local",
    "sb:push-prod": "supabase db push"
  }
}
