{
  "name": "pieshop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "preinstall": "npx only-allow pnpm",
    "db:types": "npx supabase gen types typescript --local > src/types/supabase.ts",
    "db:pull-local": "supabase db pull --local",
    "db:pull-prod": "supabase db pull --linked",
    "db:push-local": "supabase db push --local",
    "db:push-prod": "supabase db push",
    "sb:start": "supabase start",
    "sb:stop": "supabase stop"
  },
  "dependencies": {
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.42.0",
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "postcss": "^8",
    "supabase": ">=1.8.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
